    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/platform-server": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/ssr": "^20.3.2",
    "@fullcalendar/angular": "^6.1.19",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "angular-calendar": "^0.32.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "mysql2": "^3.15.0",
    "pg": "^8.10.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "^5.1.9",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^24.5.2",
    "concurrently": "^8.2.0",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "server": "node server.js",
    "start:backend": "npm run server",
    "start:frontend": "npm run start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev": "npm run start:dev",
    "install-deps": "npm install && npm install -g @angular/cli",
    "serve:ssr:dermatologia-app": "node dist/dermatologia-app/server/server.mjs",
    "build:prod": "ng build --configuration production",
    "build:prod:pages": "ng build --configuration production --base-href /dermatologia-app/",
    "build:local": "ng build --configuration production --output-path=dist-local && node ./scripts/write-env.js --dist=dist-local --apiUrl=http://localhost:3000",
    "build:web": "ng build --configuration production --base-href /dermatologia-app/ --output-path=dist-web && node ./scripts/write-env.js --dist=dist-web --apiUrl=https://nonbotanically-studentless-kasey.ngrok-free.dev/api"
  }
